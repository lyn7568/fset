<!--pages/gatewayAdmin/gatewayList.wxml-->
<import src="/template/tempList.wxml" />


<view class="weui-cells">
  <navigator url="/pages/gatewayAdmin/gatewayAdd/gatewayAdd" class="weui-cell weui-cell_access">
    <view class="weui-cell__bd">
      <text>添加设备</text>
    </view>
    <view class="weui-cell__ft left-arrow"></view>
  </navigator>
  <picker bindchange="bindCasPickerChange" value="{{casIndex1}}" range="{{casArray}}">
    <view class="weui-cell">
      <view class="weui-cell__bd">
        <text>选择设备</text>
      </view>
      <view class="weui-cell__ft">
        <view>
          <text>{{casArray[casIndex].mc}}</text>
        </view>
      </view>
    </view>
  </picker>
</view>
<view class="search-result">
  <template is="tableModel" data="{{listData:listData,th:tableTh}}" />
</view>
